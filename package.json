{
  "name": "seneca-browser",
  "version": "0.3.0",
  "description": "Seneca for the browser.",
  "main": "seneca-browser.js",
  "scripts": {
    "test": "lab -P test -I fetch,BigUint64Array,BigInt64Array,BigInt,URL,URLSearchParams",
    "prettier": "prettier --write --no-semi --single-quote seneca-browser-src.js test/*.js",
    "browserify": "browserify -o seneca-browser.js -e seneca-browser-src.js -s Seneca",
    "build": "npm run browserify",
    "smoke": "node smoke/server.js",
    "clean-yarn": "rm -rf node_modules yarn.lock",
    "clean-npm": "rm -rf node_modules package-lock.json",
    "repo-tag": "REPO_VERSION=`node -e \"console.log(require('./package').version)\"` && echo TAG: v$REPO_VERSION && git commit -a -m v$REPO_VERSION && git push && git tag v$REPO_VERSION && git push --tags",
    "repo-publish": "npm test && npm run repo-tag && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjrodger/seneca-browser.git"
  },
  "author": "Richard Rodger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rjrodger/seneca-browser/issues"
  },
  "homepage": "https://github.com/rjrodger/seneca-browser#readme",
  "dependencies": {},
  "devDependencies": {
    "@seneca/external": "^0.1.0",
    "@seneca/hapi": "^0.3.0",
    "browserify": "^16.2.3",
    "browserify-fs": "^1.0.0",
    "code": "^5.2.4",
    "hapi": "^18.1.0",
    "inert": "^5.1.3",
    "lab": "^18.0.2",
    "prettier": "^1.17.0",
    "seneca": "plugin",
    "seneca-promisify": "^0.8.0",
    "timers": "^0.1.1"
  },
  "files": [
    "README.md",
    "LICENSE",
    "seneca-browser.js"
  ]
}
